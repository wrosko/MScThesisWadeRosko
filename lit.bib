  language = {en},
  url      = {http://dx.plos.org/10.1371/journal.pone.0087830},
  urldate  = {2018-07-13},
}

@Article{Albayrak2018,
  author   = {Albayrak, Levent and Khanipov, Kamil and Golovko, George and Fofanov, Yuriy},
  title    = {Detection of multi-dimensional co-exclusion patterns in microbial communities},
  journal  = {Bioinformatics},
  year     = {2018},
  month    = jun,
  issn     = {1367-4803, 1460-2059},
  abstract = {Motivation: Identiﬁcation of complex relationships among members of microbial communities is key to understand and control the microbiota. Co-exclusion is arguably one of the most important patterns reﬂecting micro-organisms’ intolerance to each other’s presence. Knowing these relations opens an opportunity to manipulate microbiotas, personalize anti-microbial and probiotic treatments as well as guide microbiota transplantation. The co-exclusion pattern however, cannot be appropriately described by a linear function nor its strength be estimated using covariance or (negative) Pearson and Spearman correlation coefﬁcients. This manuscript proposes a way to quantify the strength and evaluate the statistical signiﬁcance of co-exclusion patterns between two, three or more variables describing a microbiota and allows one to extend analysis beyond micro-organism abundance by including other microbiome associated measurements such as, pH, temperature etc., as well as estimate the expected numbers of false positive co-exclusion patterns in a co-exclusion network.},
  doi      = {10.1093/bioinformatics/bty414},
  editor   = {Stegle, Oliver},
  file     = {Albayrak et al. - 2018 - Detection of multi-dimensional co-exclusion patter.pdf:C\:\\Users\\wrosko\\Documents\\Articles\\_Project_inspiration_papers\\_personal\\Albayrak et al. - 2018 - Detection of multi-dimensional co-exclusion patter.pdf:application/pdf;Albayrak et al. - 2018 - Detection of multi-dimensional co-exclusion patter.pdf:C\:\\Users\\wrosko\\Zotero\\storage\\3L8NYKYL\\Albayrak et al. - 2018 - Detection of multi-dimensional co-exclusion patter.pdf:application/pdf},
  language = {en},
  url      = {https://academic.oup.com/bioinformatics/advance-article/doi/10.1093/bioinformatics/bty414/5033385},
  urldate  = {2018-07-13},
}

