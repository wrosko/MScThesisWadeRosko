@Article{Tian2017,
  author   = {Tian, Liang and Wu, Ang-Kun and Friedman, Jonathan and Waldor, Matthew K. and Weiss, Scott T. and Liu, Yang-Yu},
  title    = {Deciphering {Functional} {Redundancy} in the {Human} {Microbiome}},
  journal  = {bioRxiv},
  year     = {2017},
  month    = aug,
  abstract = {The taxonomic composition of the human microbiome varies tremendously across individuals, while its gene composition or functional capacity is highly conserved across individuals1-5. Such a strikingly conserved functional capacity implies an ecological property known as functional redundancy6-9. Although this redundancy is thought to underlie the stability and resilience of the human microbiome10, 11, its origin is elusive. Here, we decipher the functional redundancy in the human microbiome by depicting its genomic content network --- a bipartite graph that links microbes to the genes in their genomes. We find that this network exhibits several topological features, such as highly-nested structure and very heterogeneous gene degree distribution, which favor high functional redundancy. Furthermore, we find that the human gut microbiota displays a unique assembly pattern --- a functional core-periphery structure, which enables it to reach a higher functional redundancy than other body sites. This unique assembly pattern and the associated high functional redundancy of the gut microbiota are gradually established during the first year of life in healthy infants. Acute disturbances (such as in Clostridium difficile infection) abolish the assembly pattern and the functional redundancy weakens, but several chronic diseases, including inflammatory bowel diseases, asthma, obesity, autism, and Parkinson’s disease, do not markedly alter the assembly pattern and the functional redundancy of the gut microbiota. This result suggests that the functional redundancy can serve as a resilience indicator of diseased microbiota against compositional interventions and hence predict the efficacy of microbiome-based therapies such as fecal microbiota transplantation. Our framework, based on the genomic content network of the human microbiome, paves the way for a mechanistic understanding of species-function relationships, which is critical for developing functional-based diagnostics and therapeutics12.},
  doi      = {10.1101/176313},
  file     = {Tian et al. - 2017 - Deciphering Functional Redundancy in the Human Mic.pdf:C\:\\Users\\wrosko\\Documents\\Articles\\_Project_inspiration_papers\\_from_stephan\\Tian et al. - 2017 - Deciphering Functional Redundancy in the Human Mic.pdf:application/pdf;Tian et al. - 2017 - Deciphering Functional Redundancy in the Human Mic.pdf:C\:\\Users\\wrosko\\Zotero\\storage\\39ZKG35R\\Tian et al. - 2017 - Deciphering Functional Redundancy in the Human Mic.pdf:application/pdf},
  language = {en},
  url      = {http://biorxiv.org/lookup/doi/10.1101/176313},
  urldate  = {2018-07-13},
}

@Article{Tang2018,
  author   = {Tang, Zheng-Zheng and Chen, Guanhua},
  title    = {Zero-inflated generalized {Dirichlet} multinomial regression model for microbiome compositional data analysis},
  journal  = {Biostatistics},
  year     = {2018},
  month    = jun,
  issn     = {1465-4644, 1468-4357},
  abstract = {There is heightened interest in using high-throughput sequencing technologies to quantify abundances of microbial taxa and linking the abundance to human diseases and traits. Proper modeling of multivariate taxon counts is essential to the power of detecting this association. Existing models are limited in handling excessive zero observations in taxon counts and in ﬂexibly accommodating complex correlation structures and dispersion patterns among taxa. In this article, we develop a new probability distribution, zero-inﬂated generalized Dirichlet multinomial (ZIGDM), that overcomes these limitations in modeling multivariate taxon counts. Based on this distribution, we propose a ZIGDM regression model to link microbial abundances to covariates (e.g. disease status) and develop a fast expectation–maximization algorithm to efﬁciently estimate parameters in the model. The derived tests enable us to reveal rich patterns of variation in microbial compositions including differential mean and dispersion. The advantages of the proposed methods are demonstrated through simulation studies and an analysis of a gut microbiome dataset.},
  doi      = {10.1093/biostatistics/kxy025},
  file     = {Tang and Chen - 2018 - Zero-inflated generalized Dirichlet multinomial re.pdf:C\:\\Users\\wrosko\\Documents\\Articles\\_Project_inspiration_papers\\_from_stephan\\Tang and Chen - 2018 - Zero-inflated generalized Dirichlet multinomial re.pdf:application/pdf;Tang and Chen - 2018 - Zero-inflated generalized Dirichlet multinomial re.pdf:C\:\\Users\\wrosko\\Zotero\\storage\\4ZVMQN9L\\Tang and Chen - 2018 - Zero-inflated generalized Dirichlet multinomial re.pdf:application/pdf},
  language = {en},
  url      = {https://academic.oup.com/biostatistics/advance-article/doi/10.1093/biostatistics/kxy025/5043450},
  urldate  = {2018-07-13},
}

